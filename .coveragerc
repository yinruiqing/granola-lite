[run]
source = app
omit = 
    app/tests/*
    app/__pycache__/*
    app/*/__pycache__/*
    app/*/migrations/*
    */venv/*
    */virtualenv/*
    */site-packages/*
    */.tox/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    
precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
title = Granola API Test Coverage Report